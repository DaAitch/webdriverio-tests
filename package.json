{
  "name": "webdriverio-tests",
  "version": "1.0.0",
  "description": "Some tests with webdriverio",
  "scripts": {
    "start": "parcel src/index.html",
    "build": "parcel build src/index.html",
    "test:dev": "cross-env NODE_ENV=development ./node_modules/webdriverio/bin/wdio ./specs/wdio.dev.conf.js",
    "test:prod": "cross-env NODE_ENV=production ./node_modules/webdriverio/bin/wdio ./specs/wdio.prod.conf.js"
  },
  "keywords": [
    "webdriver",
    "webdriverio",
    "wdio",
    "ui-testing"
