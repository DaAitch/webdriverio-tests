  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "assert": "^1.4.1",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "http-proxy-middleware": "^0.19.1",
    "parcel-bundler": "^1.10.3",
    "wdio-mocha-framework": "^0.6.4",
    "wdio-selenium-standalone-service": "0.0.12",
    "webdriverio": "^4.14.1"
  },
  "dependencies": {
    "@babel/runtime-corejs2": "^7.2.0",
    "debug": "^4.1.0"
  }
}
